# JustdialOCR SDK Configuration
# Simple setup - NO AUTHENTICATION REQUIRED

# Firebase Project Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_REGION=asia-south1

# SDK Features
ENABLE_ML_KIT=true
ENABLE_DOCUMENT_SCANNER=true
ENABLE_CAMERA=true
ENABLE_GALLERY=true
ENABLE_AUTO_DETECTION=true

# Processing Settings
OCR_TIMEOUT_MS=30000
OCR_MAX_RETRIES=3
ENABLE_FRAUD_DETECTION=true

# Image Processing
MAX_IMAGE_DIMENSION=1024
JPEG_QUALITY=85
ENABLE_IMAGE_OPTIMIZATION=true

# Development/Debug
NODE_ENV=development
DEBUG_MODE=true
ENABLE_LOGGING=true